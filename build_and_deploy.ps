# assuming your distro is Ubuntu-22.04
$env:gitroot="\\wsl$\Ubuntu-22.04/home/irismeister/git/iris-oauth2"

# assuming you have already cloned iris-oauth2
cp $env:gitroot/client/environment.prod*.ts src/environments/

ng build --configuration production --base-href=/myapp/
echo "deploying to iris-oauth2"
rm -r -fo $env:gitroot/htdocs/myapp/*
cp -fo -r dist/myapp/* $env:gitroot/htdocs/myapp/

ng build --configuration production2 --base-href=/myapp2/
echo "deploying to iris-oauth2"
rm -r -fo $env:gitroot/htdocs/myapp2/*
cp -r -fo dist/myapp/* $env:gitroot/htdocs/myapp2/

echo "done"
echo "Go to https://webgw.localdomain/myapp/ or https://webgw.localdomain/myapp2/"
